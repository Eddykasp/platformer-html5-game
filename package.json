  "bugs": {
    "url": "https://github.com/Eddykasp/platformer-html5-game/issues"
  },
  "homepage": "https://github.com/Eddykasp/platformer-html5-game#readme",
  "devDependencies": {
    "browserify": "^14.4.0",
    "eslint": "^4.1.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0",
    "watch": "^1.0.2"
  }
}
